  "repository": {
    "type": "git",
    "url": "git+https://github.com/imsnif/nmtree.git"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "nyc": "^11.3.0",
    "standard": "^10.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "commander": "^2.11.0"
  }
